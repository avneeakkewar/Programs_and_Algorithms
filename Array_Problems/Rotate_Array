class Solution {
    public int maxProfit(int[] prices) {

        int min = 0;
        int sell =0;
        int buy=prices[0];
        for(int i=1;i<prices.length;i++){
            if(prices[i] < buy) {
                buy= prices[i];
                min=i;
        }
        }
        for(int i = min; i< prices.length;i++){
            if (prices[i] > buy ) {
                sell = prices[i];
            }
        }    
return sell-buy;
}
}
